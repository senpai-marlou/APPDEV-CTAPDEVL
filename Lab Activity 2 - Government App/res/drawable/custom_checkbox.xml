<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Checked state -->
    <item>
        <bitmap android:src="@drawable/check" />
    </item>

    <!-- Unchecked state -->
    <item android:id="@android:id/mask">
        <bitmap android:src="@drawable/uncheck" />
    </item>


</layer-list>